{
	"name": "wayback",
	"version": 2,
	"github": {
		"enabled": true,
		"silent": true
	},
	"builds": [
		{
			"src": "index.js",
			"use": "@now/node-server",
			"config": {
				"maxLambdaSize": "45mb"
			}
		},
		{
			"src": "index.html",
			"use": "@now/static"
		}
	],
	"routes": [
		{
			"src": "/",
			"dest": "index.html"
		},
		{
			"src": "/(.*)",
			"dest": "index.js"
		}
	]
}
